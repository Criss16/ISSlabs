<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">

    <class name="Imprumut" table="imprumuturi">
        <id name="idImprumut" column="id_imprumut">
            <generator class="increment"/>
        </id>
        <property name="idAbonat" type="integer" column="id_abonat"/>
        <property name="idCarte" type="integer" column="id_carte"/>
        <property name="dataReturnare" type="string" column="data_ret"/>
    </class>

</hibernate-mapping>
